{
  "name": "todo",
  "version": "1.0.0",
  "description": "Simple Todo Application",
  "main": "index.js",
  "scripts": {
    "start": "node server.js --env=dev",
    "serve": "node server.js --env=dev",
    "serve:dist": "open 'http://localhost:3000' &  httpster -p 3000 -d ./dist ",

    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "copyfiles": "^0.2.1",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "httpster": "^1.0.3",
